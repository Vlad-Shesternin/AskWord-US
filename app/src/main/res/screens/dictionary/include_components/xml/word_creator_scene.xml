    <!--==============================
           SET: start
    ==============================-->
    <ConstraintSet android:id="@+id/start">

        <Constraint android:id="@+id/iv_capture">
            <Layout
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </Constraint>

        <Constraint android:id="@+id/iv_check_capture">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/iv_camera_click"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/iv_camera_click"
                app:layout_constraintTop_toTopOf="@+id/iv_camera_click"
                app:layout_constraintWidth_percent=".15" />
            <PropertySet android:visibility="invisible" />
        </Constraint>

        <Constraint android:id="@+id/iv_cancel_capture">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/iv_camera_click"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toStartOf="@+id/iv_camera_click"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/iv_camera_click"
                app:layout_constraintWidth_percent=".15" />
            <PropertySet android:visibility="invisible" />
        </Constraint>

        <Constraint android:id="@+id/iv_gallery">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/iv_camera_click"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/iv_camera_click"
                app:layout_constraintTop_toTopOf="@+id/iv_camera_click"
                app:layout_constraintWidth_percent=".15" />
            <PropertySet android:visibility="invisible" />
        </Constraint>

        <Constraint android:id="@+id/iv_camera_click">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@id/fab_back"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toEndOf="@id/ifv_prompt_add"
                app:layout_constraintStart_toStartOf="@id/ifv_prompt_add"
                app:layout_constraintTop_toTopOf="@id/fab_back"
                app:layout_constraintWidth_percent=".2" />
            <PropertySet android:visibility="invisible" />
        </Constraint>

        <Constraint android:id="@+id/iv_img_add">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toTopOf="@+id/guideH_25"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toStartOf="@+id/guideV_85"
                app:layout_constraintStart_toEndOf="@+id/guideV_15"
                app:layout_constraintTop_toTopOf="parent" />
            <Transform android:elevation="5dp"/>
        </Constraint>

        <Constraint android:id="@+id/view_curtain">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/preview"
                app:layout_constraintEnd_toEndOf="@+id/preview"
                app:layout_constraintStart_toStartOf="@+id/preview"
                app:layout_constraintTop_toTopOf="@+id/preview" />
        </Constraint>

        <Constraint android:id="@+id/preview">
            <Layout
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </Constraint>

        <Constraint android:id="@+id/edit_word">
            <Layout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toTopOf="@+id/guideH_40"
                app:layout_constraintEnd_toStartOf="@+id/guideV_85"
                app:layout_constraintStart_toEndOf="@+id/guideV_15"
                app:layout_constraintTop_toBottomOf="@+id/guideH_25" />
        </Constraint>

        <Constraint android:id="@+id/edit_translation">
            <Layout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toTopOf="@+id/guideH_55"
                app:layout_constraintEnd_toStartOf="@+id/guideV_85"
                app:layout_constraintStart_toEndOf="@+id/guideV_15"
                app:layout_constraintTop_toBottomOf="@+id/guideH_40" />
        </Constraint>

        <Constraint android:id="@+id/tv_translation">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/edit_translation"
                app:layout_constraintEnd_toEndOf="@+id/edit_translation"
                app:layout_constraintStart_toStartOf="@+id/edit_translation"
                app:layout_constraintTop_toTopOf="@id/edit_translation" />
        </Constraint>

        <Constraint android:id="@+id/ifv_list_translations">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toTopOf="@+id/guideH_55"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toStartOf="parent"
                app:layout_constraintHeight_percent="0.07"
                app:layout_constraintTop_toBottomOf="@+id/guideH_40" />
            <PropertySet android:visibility="invisible" />
            <CustomAttribute
                app:attributeName="crossfade"
                app:customFloatValue="0.0" />
        </Constraint>

        <Constraint android:id="@+id/ib_translations">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toTopOf="@+id/guideH_55"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintHeight_percent="0.07"
                app:layout_constraintStart_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/guideH_40" />
            <PropertySet android:visibility="invisible" />
        </Constraint>

        <Constraint android:id="@+id/fab_add">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintHorizontal_bias="0.95"
                app:layout_constraintStart_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.8"
                app:layout_constraintWidth_percent="0.15" />
            <PropertySet android:visibility="invisible" />
        </Constraint>

        <Constraint android:id="@+id/fab_back">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.05"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.8"
                app:layout_constraintWidth_percent="0.15" />
            <Transform android:elevation="@dimen/dictionary_fab_elevation" />
        </Constraint>

        <Constraint android:id="@+id/ifv_prompt_add">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toStartOf="@+id/guideV_85"
                app:layout_constraintStart_toEndOf="@+id/guideV_15"
                app:layout_constraintTop_toBottomOf="@+id/guideH_55"
                app:layout_constraintWidth_percent="0.2" />
            <CustomAttribute
                app:attributeName="crossfade"
                app:customFloatValue="0.0" />
        </Constraint>

        <Constraint android:id="@+id/layout_translations">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/ifv_list_translations"
                app:layout_constraintEnd_toEndOf="@+id/ifv_list_translations"
                app:layout_constraintStart_toStartOf="@+id/ifv_list_translations"
                app:layout_constraintTop_toTopOf="@+id/ifv_list_translations" />
            <PropertySet android:visibility="invisible" />
        </Constraint>

        <Constraint android:id="@+id/layout_prompt">
            <Layout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/guideH_15"
                app:layout_constraintWidth_percent="0.999" />
            <PropertySet android:visibility="invisible" />
        </Constraint>
    </ConstraintSet>

